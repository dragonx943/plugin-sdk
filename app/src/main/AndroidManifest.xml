        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        tools:ignore="DataExtractionRules">

        <provider
            android:name="app.kotatsu.parser.mimihentai.MimiHentaiContentProvider"
