            android:enabled="true"
            android:exported="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            tools:ignore="ExportedContentProvider">

            <intent-filter>
                <action android:name="app.kotatsu.parser.PROVIDE_MANGA" />
            </intent-filter>
        </provider>
    </application>

</manifest>